class Solution:
    def countSubstrings(self, s: str) -> int:
        res=[]
        for i in range(0,len(s)):
            for j in range(i+1,len(s)+1):
                res.append(s[i:j])
        count=0
        for i in res:
            if self.palin(i)==True:
                count=count+1
        return count

        
    
    def palin(self,x):
        left=0
        right=len(x)-1
        while left<=right:
            if x[left]==x[right]:
                right=right-1
                left=left+1
            else:
                return False
        return True


                
